{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "aw-studio/laravel-states": "^1.3",
        "based/laravel-typescript": "^0.0.3",
        "based/momentum-modal": "^0.1.5",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.5.4",
        "jdavidbakr/mail-tracker": "^6.0",
        "laravel/breeze": "^1.10",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^2.8",
        "laravel/scout": "^9.4",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "singlestoredb/singlestoredb-laravel": "^1.0",
        "spatie/elasticsearch-search-string-parser": "^1.2",
        "spatie/laravel-artisan-dispatchable": "^1.3",
        "spatie/laravel-ciphersweet": "^1.0",
        "spatie/laravel-data": "^2.0",
        "spatie/laravel-options": "^1.0",
        "spatie/laravel-sql-commenter": "^1.1",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "pkboom/laravel-dump-server": "^0.0.4",
        "pkboom/laravel-grab": "^0.0.2",
        "pkboom/laravel-tinker-on-vscode": "^0.2.20",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
